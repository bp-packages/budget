{
    "name": "bp-packages/budget",
    "description": "CRUD budget",
    "type": "library",
    "license": "MIT",
    "require": {
        "codeigniter4/appstarter": "4",
        "composer/installers": "~1.0",
        "dompdf/dompdf": "^1.2"
    },
    "authors": [
        {
            "name": "leomaciel",
            "email": "dev@leomaciel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BpPackages\\Budget\\": "app/Libraries"
        }
    },
    "extra": {
        "installer-paths": {
            "app/Libraries/{$name}": [
